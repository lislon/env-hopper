{
  "name": "@env-hopper/backend-core",
  "version": "2.0.1-alpha",
  "description": "Backend core library for Env Hopper",
  "homepage": "https://github.com/lislon/env-hopper",
  "repository": {
    "type": "git",
    "url": "https://github.com/lislon/env-hopper.git",
    "directory": "packages/backend-core"
  },
  "license": "MIT",
  "author": "Igor Golovin",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "my-custom-condition": "./src/index.ts",
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "premove ./dist ./coverage ./dist-ts",
    "compile": "tsc --build",
    "dev": "tsx watch src/index.ts",
    "prisma:generate": "prisma generate",
    "test:eslint": "eslint ./src",
    "test:unit": "vitest",
    "test:unit:dev": "pnpm run test:unit --watch"
  },
  "dependencies": {
    "@kubernetes/client-node": "^1.2.0",
    "@prisma/client": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "cors": "^2.8.5",
    "dotenv-defaults": "catalog:",
    "express": "catalog:",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "yaml": "^2.8.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/dotenv-defaults": "catalog:",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "catalog:",
    "esbuild": "catalog:",
    "prisma": "^6.8.2",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vite-plugin-dts": "^4.5.4"
  },
  "engines": {
    "node": ">=16"
  }
}
