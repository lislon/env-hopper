{
  "name": "@env-hopper/frontend-build-vite",
  "version": "2.0.1-alpha.0",
  "description": "Vite build config for Env Hopper frontend",
  "homepage": "https://github.com/lislon/env-hopper",
  "repository": {
    "type": "git",
